/* tab样式覆盖 */
.table-index--helper-tabs-nav .van-tabs__line {
    bottom: 4px;
}
.table-index--helper-notab>van-sticky>.van-sticky {
    height: 0 !important;
}
/* 覆盖微信默认按钮样式与vant一致 */

button {
    position: relative;
    -webkit-align-items: center;
    align-items: center;
    -webkit-justify-content: center;
    justify-content: center;
    box-sizing: border-box;
    padding: 0 15px;
    text-align: center;
    vertical-align: middle;
    -webkit-appearance: none;
    -webkit-text-size-adjust: 100%;
    font-size: 14px;
    font-size: var(--button-default-font-size, 14px);
    transition: opacity .2s;
    transition: opacity var(--animation-duration-fast, .2s);
    border-radius: 2px;
    border-radius: var(--button-border-radius, 2px);
    height: 44px;
    height: var(--button-default-height, 44px);
    line-height: 42px;
    line-height: var(--button-line-height, 42px);
}

button:before {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 100%;
    height: 100%;
    border: inherit;
    border-radius: inherit;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    opacity: 0;
    content: " ";
    background-color: #000;
    background-color: var(--black, #000);
    border-color: #000;
    border-color: var(--black, #000);
}

button:after {
    border-width: 0;
}

button[disabled] {
    opacity: var(--button-disabled-opacity, .5);
}

button[type=default],
button[loading][type=default],
button[disabled][type=default],
.button-hover[type=default] {
    background-color: var(--color-default);
    color: #fff;
    border: var(--button-border-width, 1px) solid var(--button-default-border-color, var(--color-default))
}

button[type=primary],
button[loading][type=primary],
button[disabled][type=primary],
.button-hover[type=primary] {
    background-color: var(--color-primary);
    color: #fff;
    border: var(--button-border-width, 1px) solid var(--button-primary-border-color, var(--color-primary))
}

button[type=warning],
button[loading][type=warning],
button[disabled][type=warning],
.button-hover[type=warning] {
    background-color: var(--color-warning);
    color: #fff;
    border: var(--button-border-width, 1px) solid var(--button-warning-border-color, var(--color-warning))
}

button[type=danger],
button[loading][type=danger],
button[disabled][type=danger],
.button-hover[type=danger] {
    background-color: var(--color-danger);
    color: #fff;
    border: var(--button-border-width, 1px) solid var(--button-danger-border-color, var(--color-danger))
}

button[type=info],
button[loading][type=info],
button[disabled][type=info],
.button-hover[type=info] {
    background-color: var(--color-info);
    color: #fff;
    border: var(--button-border-width, 1px) solid var(--button-info-border-color, var(--color-info))
}

button[type=dark],
button[loading][type=dark],
button[disabled][type=dark],
.button-hover[type=dark] {
    background-color: var(--color-dark);
    color: #fff;
    border: var(--button-border-width, 1px) solid var(--button-dark-border-color, var(--color-dark))
}

button[type=reset],
button[disabled][type=reset],
.button-hover[type=reset],
.van-button--reset {
    background-color: var(--color-lightgray);
    color: #fff;
    border: var(--button-border-width, 1px) solid var(--button-reset-border-color, var(--color-lightgray))
}

button[type=blank],
button[disabled][type=blank],
.button-hover[type=blank],
.van-button--blank {
    background-color: #fff;
    color: gray;
    border: var(--button-border-width, 1px) solid #fff
}

button[size=large] {
    width: 100%;
    height: 50px;
    height: var(--button-large-height, 50px);
    line-height: var(--button-large-height, 50px);
}

button[size=small] {
    min-width: 60px;
    min-width: var(--button-small-min-width, 60px);
    height: 30px;
    height: var(--button-small-height, 30px);
    padding: 0 8px;
    padding: 0 var(--padding-xs, 8px);
    font-size: 12px;
    font-size: var(--button-small-font-size, 12px)
}

button[size=mini] {
    display: inline-block;
    min-width: 50px;
    min-width: var(--button-mini-min-width, 50px);
    height: 22px;
    height: var(--button-mini-height, 22px);
    font-size: 10px;
    font-size: var(--button-mini-font-size, 10px)
}

.button-hover:before {
    opacity: .15
}

button .van-loading {
    --loading-spinner-color: #fff;
    --loading-text-color: #fff;
}

/* vant button 增加dark颜色 */
.van-button--dark {
    color: #fff;
    background: var(--color-dark);
    border: var(--button-border-width, 1px) solid var(--button-dark-border-color, var(--color-dark))
}

.van-button--plain.van-button--dark {
    color: var(--button-dark-background-color)
}

/* 增加dialog样式 .helper-dialog */
.dialog-index--helper-dialog .dialog-index--van-dialog__confirm {
    color: var(--color-primary) !important;
    background-color: #fff;
}

.dialog-index--helper-dialog .dialog-index--van-dialog__cancel {
    background-color: #fff;
}

.dialog-index--helper-dialog .van-dialog__message-text {
    font-size: 16px;
}
/* tag样式类 */
.van-tag--flag-recommend,
.van-tag--status-hidden,
.van-tag--status-0 {
    background-color: var(--color-danger);
}
.van-tag--status-normal,
.van-tag--status-1 {
    background-color: var(--color-primary);
}
.van-tag--flag-hot,
.van-tag--status-2 {
    background-color: var(--color-warning);
}
.van-tag--purple,
.van-tag--status-3 {
    background-color: var(--color-purple);
}
.van-tag--status-4 {
    background-color: var(--color-info);
}
.van-tag--status-5 {
    background-color: var(--color-dark);
}

/* 增加圆角输入框样式 .helper-round-input */
.helper-round-input {
    background-color: var(--color-whitesmoke);
    border-radius: 2px;
    margin: 20rpx 0;
}

.helper-round-input .van-cell {
    --cell-vertical-padding: 5px;
    --cell-horizontal-padding: 5px;
    --cell-background-color: none;
    --cell-line-height: 34px;
}

.helper-round-input .van-field__input {
    padding-left: 20px !important;
}

.helper-round-input .van-field__clear-root {
    margin-right: 5px;
}

/* 增加上拉多选菜单样式 .helper-popup-selects__title */
.helper-popup-selects__title {
    text-align: center;
    padding: 16px;
    padding: var(--padding-md, 16px);
    color: #646566;
    color: var(--action-sheet-description-color, #646566);
    font-size: 14px;
    font-size: var(--action-sheet-description-font-size, 14px);
    line-height: 20px;
    line-height: var(--action-sheet-description-line-height, 20px);
}

.helper-popup-selects__toolbar {
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    height: 44px;
    height: var(--picker-toolbar-height, 44px);
    line-height: 44px;
    line-height: var(--picker-toolbar-height, 44px);
}

.helper-popup-selects__cancel,
.helper-popup-selects__confirm {
    padding: 0 16px;
    padding: var(--picker-action-padding, 0 16px);
    font-size: 14px;
    font-size: var(--picker-action-font-size, 14px);
    color: var(--picker-action-text-color);
}

/* editor */
.editor-container {
    width: 100%;
}

.ql-container {
    box-sizing: border-box;
    width: 100%;
    height: 100%;
    font-size: 16px;
    line-height: 1.5;
    overflow: auto;
    padding: 10px 10px 20px 10px;
}

.ql-active {
    color: #18bc9c !important;
}

.editor-toolbar .iconfont {
    display: inline-block;
    width: 30px;
    height: 30px;
    cursor: pointer;
    font-size: 20px;
    color: var(--color-dark);
}

.editor-toolbar {
    box-sizing: border-box;
    padding: 0 10px;
    min-height: 50px;
    width: 100%;
    position: fixed;
    left: 0;
    right: 100%;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-left: none;
    border-right: none;
    background-color: #fff;
}

.editor-toolbar scroll-view {
    white-space: nowrap;
}

.van-cell__title {
    color: #969799;
}

.van-cell__value {
    color: var(--color-dark) !important;
    font-size: 14px !important;
}

/* 加载动画 */
.page-loading {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    left: 0;
    top: 0;
    background-color: #fff;
    z-index: 100;
}
.helper-spinner {
    width: 40px;
    height: 40px;
    position: relative;
    margin: 100px auto;
}
.double-bounce1,
.double-bounce2 {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    background-color: var(--color-primary);
    opacity: 0.6;
    position: absolute;
    top: 0;
    left: 0;

    -webkit-animation: sk-bounce 2.0s infinite ease-in-out;
    animation: sk-bounce 2.0s infinite ease-in-out;
}

.double-bounce2 {
    -webkit-animation-delay: -1.0s;
    animation-delay: -1.0s;
}

@-webkit-keyframes sk-bounce {

    0%,
    100% {
        -webkit-transform: scale(0.0)
    }

    50% {
        -webkit-transform: scale(1.0)
    }
}

@keyframes sk-bounce {

    0%,
    100% {
        transform: scale(0.0);
        -webkit-transform: scale(0.0);
    }

    50% {
        transform: scale(1.0);
        -webkit-transform: scale(1.0);
    }
}

/* 全屏错误提示 */
.page-error {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: absolute;
    left: 0;
    top: 0;
    background-color: #fff;
    z-index: 100;
    color: lightgray;
}
.page-error .van-icon {
    margin-bottom: 20px;
    color: var(--color-danger);
}

